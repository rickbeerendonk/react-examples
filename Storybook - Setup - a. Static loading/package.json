{
  "name": "react-storybook-hmr-eslint-webpack-babel",
  "description": "Using React with Storybook (Static), Hot Module Replacement, ESLint, Webpack & Babel",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickbeerendonk/react-examples.git"
  },
